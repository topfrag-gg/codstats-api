{
	"name": "codstats-api",
	"displayName": "CODStats API",
	"version": "0.1.0",
	"private": true,
	"description": "API for tracking Call of Duty League and Challengers statistics",
	"author": {
		"name": "mister-fix",
		"url": "https://github.com/mister-fix/"
	},
	"license": "MIT",
	"homepage": "https://github.com/mister-fix/codstats-api#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mister-fix/codstats-api.git"
	},
	"bugs": {
		"url": "https://github.com/mister-fix/codstats-api/issues"
	},
	"keywords": [
		"codstats",
		"call-of-duty",
		"call-of-duty-league",
		"cdl",
		"challengers",
		"esports",
		"stats",
		"statistics",
		"gaming-api",
		"esports-api",
		"cod-api",
		"express",
		"typescript",
		"nodejs",
		"rest-api",
		"multiplayer",
		"leaderboards",
		"matches",
		"players",
		"teams"
	],
	"type": "module",
	"main": "dist/index.js",
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=9"
	},
	"exports": {
		".": "./dist/index.js"
	},
	"scripts": {
		"prepare": "husky",
		"build": "node scripts/build.js",
		"lint": "eslint .",
		"lint:all": "npm-run-all --parallel lint spellcheck",
		"lint:fix": "eslint --fix .",
		"prettier:check": "prettier --check .",
		"prettier:fix": "prettier --write .",
		"format": "npm run lint:fix && npm run prettier:fix",
		"spellcheck": "cspell ** --dot",
		"dev": "cross-env NODE_ENV=development nodemon",
		"start": "cross-env NODE_ENV=production node dist/index.js",
		"start:prod": "npm run build && npm run start",
		"type-check": "tsc --noEmit",
		"clean": "rimraf dist .tsbuildinfo",
		"reset": "npm run clean && npm install && npm run build",
		"deploy": "flyctl deploy",
		"docs": "open-cli https://github.com/topfrag-gg/codstats-api#readme"
	},
	"lint-staged": {
		"{index.ts, src/**/*.ts}": [
			"eslint --fix --cache --cache-location node_modules/.cache/eslint",
			"prettier --write"
		],
		"*.{js,cjs,mjs}": [
			"eslint --fix --cache --cache-location node_modules/.cache/eslint",
			"prettier --write"
		],
		"**/*.{json,jsonc,md,yml,yaml}": ["prettier --write"],
		"!**/.prettierrc": []
	},
	"dependencies": {
		"compression": "1.8.0",
		"config": "3.3.12",
		"cors": "2.8.5",
		"cross-env": "7.0.3",
		"dotenv": "16.5.0",
		"express": "5.1.0",
		"express-rate-limit": "7.5.0",
		"helmet": "8.1.0",
		"http-errors": "2.0.0",
		"morgan": "1.10.0",
		"serve-favicon": "2.5.0",
		"tsconfig-paths": "4.2.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.8.0",
		"@commitlint/config-conventional": "^19.8.0",
		"@commitlint/format": "^19.8.0",
		"@cspell/dict-filetypes": "^3.0.12",
		"@cspell/dict-typescript": "3.2.1",
		"@eslint/css": "0.7.0",
		"@eslint/js": "9.25.1",
		"@eslint/json": "0.12.0",
		"@eslint/markdown": "6.4.0",
		"@flydotio/dockerfile": "0.7.10",
		"@types/compression": "1.7.5",
		"@types/config": "3.3.5",
		"@types/cors": "2.8.17",
		"@types/express": "5.0.1",
		"@types/http-errors": "2.0.4",
		"@types/morgan": "1.9.9",
		"@types/node": "22.15.14",
		"@types/serve-favicon": "2.5.7",
		"cspell": "^9.0.0",
		"cspell-dictionary": "^9.0.0",
		"eslint": "9.25.1",
		"globals": "14.0.0",
		"husky": "^9.1.7",
		"lint-staged": "15.5.1",
		"nodemon": "3.1.9",
		"npm-run-all": "4.1.5",
		"open-cli": "8.0.0",
		"prettier": "3.5.3",
		"prettier-plugin-jsdoc": "1.3.2",
		"pretty-quick": "4.1.1",
		"rimraf": "6.0.1",
		"ts-node": "10.9.2",
		"ts-patch": "3.3.0",
		"tsc-alias": "1.8.15",
		"tslib": "2.8.1",
		"typescript": "5.8.3",
		"typescript-eslint": "8.29.1",
		"typescript-transform-paths": "3.5.5"
	},
	"contributors": [
		{
			"name": "mister-fix",
			"url": "https://github.com/mister-fix"
		}
	]
}
