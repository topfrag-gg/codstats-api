{
	"name": "codstats-api",
	"displayName": "CODStats API",
	"version": "0.0.0",
	"private": true,
	"description": "API for tracking Call of Duty League and Challengers statistics",
	"author": {
		"name": "mister-fix",
		"url": "https://github.com/mister-fix/"
	},
	"license": "MIT",
	"homepage": "https://github.com/mister-fix/codstats-api#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mister-fix/codstats-api.git"
	},
	"bugs": {
		"url": "https://github.com/mister-fix/codstats-api/issues"
	},
	"keywords": [
		"codstats",
		"call-of-duty",
		"call-of-duty-league",
		"cdl",
		"challengers",
		"esports",
		"stats",
		"statistics",
		"gaming-api",
		"esports-api",
		"cod-api",
		"express",
		"typescript",
		"nodejs",
		"rest-api",
		"multiplayer",
		"leaderboards",
		"matches",
		"players",
		"teams"
	],
	"type": "module",
	"scripts": {
		"prepare": "husky",
		"lint": "eslint .",
		"lint:all": "npm-run-all --parallel lint spellcheck",
		"lint:fix": "eslint --fix .",
		"prettier:check": "prettier --check .",
		"prettier:fix": "prettier --write .",
		"format": "npm run lint:fix && npm run prettier:fix",
		"spellcheck": "cspell ** --dot"
	},
	"lint-staged": {
		"*.{js,cjs,mjs}": [
			"eslint --fix --cache --cache-location node_modules/.cache/eslint",
			"prettier --write"
		],
		"**/*.{json,jsonc,md,yml,yaml}": ["prettier --write"],
		"!**/.prettierrc": []
	},
	"devDependencies": {
		"@commitlint/cli": "^19.8.0",
		"@commitlint/config-conventional": "^19.8.0",
		"@commitlint/format": "^19.8.0",
		"@cspell/dict-filetypes": "^3.0.12",
		"@eslint/css": "0.7.0",
		"@eslint/js": "9.25.1",
		"@eslint/json": "0.12.0",
		"@eslint/markdown": "6.4.0",
		"cspell": "^9.0.0",
		"cspell-dictionary": "^9.0.0",
		"eslint": "9.25.1",
		"globals": "14.0.0",
		"husky": "^9.1.7",
		"lint-staged": "15.5.1",
		"npm-run-all": "4.1.5",
		"prettier": "3.5.3",
		"prettier-plugin-jsdoc": "1.3.2",
		"pretty-quick": "4.1.1",
		"typescript-eslint": "8.29.1"
	}
}
